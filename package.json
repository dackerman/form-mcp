{
  "name": "mcp-form-server",
  "version": "1.0.0",
  "description": "MCP server for creating HTML forms and collecting responses",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-form-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "clean": "rm -rf dist",
    "test": "pnpm build && vitest",
    "test:ui": "pnpm build && vitest --ui",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "forms",
    "html",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "express": "^4.19.2",
    "uuid": "^10.0.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.4.2",
    "typescript": "^5.4.5",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}